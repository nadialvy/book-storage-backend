{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "indent": "off",
        "no-console": "off",
        "no-unneeded-ternary": "off",
        "quotes": "off",
        "no-else-return": "off"
    }
}
